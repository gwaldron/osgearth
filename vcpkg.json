{
    "name": "osgearth",
    "version-string": "3.7.3",
    "port-version": 1,
    "description": "osgEarth - 3D Maps and Terrain - Copyright 2025 Pelican Mapping",
    "homepage": "https://github.com/gwaldron/osgearth",
    "supports": "!(x86 | wasm32)",
    "dependencies": [
        "blend2d",
        "draco",
        { "name": "gdal",
          "default-features": false,
          "features": [ "recommended-features", "jpeg", "tools" ] },
        "geos",
        "libwebp",
        "libzip",
        "opengl",
        "osg",
        "protobuf",
        "sqlite3",
        "rocksdb",
        "spdlog",
        "meshoptimizer" 
    ]
}
